{
  "name": "prestashop/contextual-help-api",
  "type": "project",
  "description": "Contextual Help for PrestaShop",
  "homepage": "https://github.com/PrestaShop/contextual-help-api",
  "license": "MIT",
  "require": {
    "php": ">=8.0.7",
    "ezyang/htmlpurifier": "^4.13",
    "guzzlehttp/guzzle": "^7.3",
    "monolog/monolog": "^2.3",
    "symfony/dotenv": "^5.3",
    "twig/twig": "^3.3"
  },
  "require-dev": {
    "behat/behat": "^3.8",
    "nikic/php-parser": "^4.11",
    "phpstan/phpstan": "^0.12.91",
    "phpunit/phpunit": "^9.5",
    "prestashop/php-dev-tools": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Help\\PrestaShop\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.0.7"
    },
    "sort-packages": true
  },
  "authors": [
    {
      "name": "PrestaShop SA",
      "email": "contact@prestashop.com"
    }
  ],
  "prefer-stable": true
}
